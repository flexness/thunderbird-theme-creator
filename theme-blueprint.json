{
  "layout": {
    "main": {
      "type": "container",
      "children": ["toolbar", "tabs", "content", "statusBar"]
    },
    "toolbar": {
      "type": "component",
      "componentType": "Toolbar"
    },
    "tabs": {
      "type": "container",
      "children": ["tab1", "tab2"]
    },
    "tab1": {
      "type": "component",
      "componentType": "Tab"
    },
    "tab2": {
      "type": "component",
      "componentType": "Tab"
    },
    "content": {
      "type": "container",
      "children": ["sidebar", "messagePane"]
    },
    "sidebar": {
      "type": "component",
      "componentType": "Sidebar"
    },
    "messagePane": {
      "type": "component",
      "componentType": "MessagePane"
    },
    "statusBar": {
      "type": "component",
      "componentType": "Statusbar"
    }
  },
  "colors": {
    "frame": { "type": "color", "default": "#2f343e" },
    "frame_inactive": { "type": "color", "default": "#2f343e" },
    "icons": { "type": "color", "default": "#5294E2" },
    "icons_attention": { "type": "color", "default": "#ffcc00" },
    "sidebar": { "type": "color", "default": "#2f343e" },
    "sidebar_border": { "type": "color", "default": "#CCCCCC" },
    "sidebar_highlight": { "type": "color", "default": "#5294E2" },
    "sidebar_highlight_border": { "type": "color", "default": "#ffcc00" },
    "sidebar_highlight_text": { "type": "color", "default": "#cccccc" },
    "sidebar_text": { "type": "color", "default": "#cccccc" },
    "toolbar": { "type": "color", "default": "#383C4A" },
    "toolbar_field": { "type": "color", "default": "#FFFFFF" },
    "toolbar_field_text": { "type": "color", "default": "#5C616C" },
    "toolbar_field_border": { "type": "color", "default": "#CFD6E6" },
    "toolbar_field_border_focus": { "type": "color", "default": "#ffcc00" },
    "toolbar_field_highlight": { "type": "color", "default": "#5294E2" },
    "toolbar_field_highlight_text": { "type": "color", "default": "#FFFFFF" },
    "toolbar_field_separator": { "type": "color", "default": "#000000" },
    "toolbar_field_focus": { "type": "color", "default": "#FFFFFF" },
    "toolbar_field_text_focus": { "type": "color", "default": "#000000" },
    "toolbar_text": { "type": "color", "default": "#F5F5F5" },
    "toolbar_top_separator": { "type": "color", "default": "#272A34" },
    "toolbar_bottom_separator": { "type": "color", "default": "#DCDFE3" },
    "toolbar_vertical_separator": { "type": "color", "default": "#5C616C" },
    "toolbar_bottom_separator_inactive": { "type": "color", "default": "#000000" },
    "button_background_hover": { "type": "color", "default": "#5294E2" },
    "button_background_active": { "type": "color", "default": "#5294E2" },
    "popup": { "type": "color", "default": "#FFFFFF" },
    "popup_border": { "type": "color", "default": "#DCDFE3" },
    "popup_highlight": { "type": "color", "default": "#5294E2" },
    "popup_highlight_text": { "type": "color", "default": "#FFFFFF" },
    "popup_text": { "type": "color", "default": "#5C616C" },
    "tab_background_separator": { "type": "color", "default": "rgba(255, 255, 255, 0.5)" },
    "tab_background_text": { "type": "color", "default": "#AFB8C6" },
    "tab_line": { "type": "color", "default": "#5294E2" },
    "tab_loading": { "type": "color", "default": "#ffcc00" },
    "tab_selected": { "type": "color", "default": "#ffcc00" },
    "tab_text": { "type": "color", "default": "#000000" },
    "bookmark_text": { "type": "color", "default": "#ebdbb2" },
    "ntp_background": { "type": "color", "default": "#ebdbb2" },
    "ntp_text": { "type": "color", "default": "#ebdbb2" }

  },
  "images": {
    "theme_frame": { "type": "image", "default": "" },
    "additional_backgrounds": { "type": "image", "default": "" }
  },
  "properties": {
    "additional_backgrounds_alignment": { "type": "select", "default": "center" },
    "additional_backgrounds_tiling": { "type": "select", "default": "no-repeat" },
    "color_scheme": { "type": "select", "default": "light" },
    "content_color_scheme": { "type": "select", "default": "auto" },
    "dark_color_scheme": { "type": "select", "default": "dark" },
    "light_color_scheme": { "type": "select", "default": "light" }
  }
}


